{
  "name": "mf-react-rsbuild",
  "version": "1.0.0",
  "description": "Rsbuild Module Federation example with Zephyr",
  "private": true,
  "scripts": {
    "build": "pnpm build-provider && pnpm build-consumer",
    "build-provider": "pnpm --filter=mf-react-rsbuild-provider build",
    "build-consumer": "pnpm --filter=mf-react-rsbuild-consumer build",
    "dev": "./start-with-zephyr.sh",
    "dev:no-auth": "./start-dev.sh"
  },
  "keywords": ["rsbuild", "module-federation", "zephyr"],
  "author": "",
  "license": "MIT"
}
